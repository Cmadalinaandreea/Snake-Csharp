<Application x:Class="Snake.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Snake"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#002F4F</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#005E9F</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#4f4867</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#ffc5f5</SolidColorBrush>
        <SolidColorBrush x:Key="OverlayColor">#ffe1fe</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Bebas Neue - Regular</FontFamily>
    </Application.Resources>
</Application>
